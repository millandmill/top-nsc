img{ border:0;}

.cleaner {clear:both; width:100%; height:0px;}
.h30{clear:both; width:100%; height:30px;}
.h20{clear:both; width:100%; height:20px;}
.h10{clear:both; width:100%; height:10px;}
.h5{clear:both; width:100%; height:5px;}
.space20{ padding-left:20px;}
.space30{ padding-left:30px;}
.space40{ padding-left:40px;}

/*-----FONT-----////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////*/

.font8{ font-size:0.8em !important}
.font10{ font-size:1em}
.font12{ font-size:1.2em}
.font14{ font-size:1.4em}
.font16{ font-size:1.6em}
.font18{ font-size:1.8em}
.font20{ font-size:2em}
.font22{ font-size:2.2em}
.font24{ font-size:2.4em}
.font26{ font-size:2.6em}
.font28{ font-size:2.8em}
.font30{ font-size:3em}
/*-----COLOR-----////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////*/

.white{color:#FFFFFF;}
.grey{color:#5B5B5B;}
.black{color:#000000;}
.orange{color:#EF9540;}
.green{color:#3AD17A;}
.red{color:#E74C3C;}
body{
  font-family: 'Boon-0.6'; 
}
/*-----LOGIN-----////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////*/

.navbar-inverse{
	font-family: 'Boon-0.6';
	background:#0f295a !important;	
}

.navbar-inverse .navbar-brand {
	font-family: 'Boon-0.6';
	font-size: 1.6em;
    color: #fff !important;
}

.navbar-inverse .navbar-brand img{
  margin-top: -15px;
}

.form-login{
	font-family: 'Boon-0.6';
	background: #eceef1;
	border-radius: 10px;
	margin:10em 2em 2em 2em;
	padding: 10px 0px;
	
}
.form-login ul{
	list-style:none;
	margin: 0;
    padding: 1.4em 1em;
}

.form-login input[type=text], .form-login input[type=password]{
	width:100%;
	font-size:1.8em;
	background:#dde3ec;
	border:1px solid #dde3ec;
	margin:0.2em 0;
	padding:0 0.5em;
}
.form-login input[type=text]:focus, .form-login input[type=password]:focus{
	outline:none;
	border:1px solid #ffd7a9;
}

.bt-login {
    font-size: 1.6em;
    font-weight: bold;
    border: 2px solid #EF9540;
    color: #fff;
    background: #EF9540;
    padding: 0.1em 1em;
}
.bt-login:hover, .bt-login:focus{
	background:#e37b00;
	border:2px solid #e37b00;
	outline:none;
}

#footer{
  text-align: center;
}
/*-----PROFILE-----////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////*/
.war{
	padding-left:0px !important;
}

.nav-side-menu {
  overflow: auto;
  background-color: #ecf7fb !important; 
  position: fixed;
  top: 0px;
  width: 300px;
  height: 100%;
  color: #000;
}
.nav-side-menu .brand {
  background-color: #0f295a;
  line-height: 50px;
  display: block;
  text-align: center;
  font-size: 14px;
  color:#ffffff;
}
.nav-side-menu .brand a{
  color:#ffffff;
}
.nav-side-menu .toggle-btn {
  display: none;
}
.nav-side-menu ul,
.nav-side-menu li {
  background-color: #ecf7fb; 
  list-style: none;
  padding: 0px;
  margin: 0px;
  line-height: 35px;
  cursor: pointer;
}

.nav-side-menu ul .sub-menu li,
.nav-side-menu li .sub-menu li {
  background-color: #2ebbe5;
  border: none;
  line-height: 28px;
  border-bottom: 1px solid #002732;
  margin-left: 0px;
}
.nav-side-menu ul .sub-menu li:hover,
.nav-side-menu li .sub-menu li:hover {
  background-color: #4f5b69;
}

.nav-side-menu li {
  padding-left: 5px;
  border-left: 3px solid #002732;
  border-bottom: 1px solid #002732;
}
.nav-side-menu li a {
  text-decoration: none;
  color: #000;
}
.nav-side-menu li a i {
  padding-left: 10px;
  width: 20px;
  padding-right: 20px;
}
.nav-side-menu li:hover {
  border-left: 3px solid #d19b3d;
  background-color: #4f5b69;
  -webkit-transition: all 1s ease;
  -moz-transition: all 1s ease;
  -o-transition: all 1s ease;
  -ms-transition: all 1s ease;
  transition: all 1s ease;
}

.head-user{
  color: #fff !important; 
  padding:10px 20px 0 0 ;
  line-height:10px; 
}

.menu-list a{
	font-family: 'Boon-0.6';
	font-size:1.2em;
	
}

.content{
	margin-top:60px;
	font-family: 'Boon-0.6';	
}

/*-----REGISTER-----////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////*/
.form-register{
  font-family: 'Boon-0.6';
  background: #eceef1;
  border-radius: 10px;
  margin:6em 2em 2em 2em;
  padding: 10px 10px;
  
}

.form-register ul{
  list-style:none;
  margin: 0;
    padding: 0.2em 1em;
}

.form-register input[type=text], .form-register input[type=password]{
  width:100%;
  font-size:1.8em;
  background:#dde3ec;
  border:1px solid #dde3ec;
  margin:0.2em 0;
  padding:0 0.5em;
}
.form-register input[type=text]:focus, .form-register input[type=password]:focus{
  outline:none;
  border:1px solid #ffd7a9;
}

.bt-submit {
    font-size: 1.2em;
    /*font-weight: bold;*/
    border: 2px solid #EF9540;
    color: #fff;
    background: #EF9540;
    padding: 0.1em 1em;
}
.bt-submit:hover, .bt-submit:focus{
  background:#e37b00;
  border:2px solid #e37b00;
  outline:none;
}

.bt-reset {
    font-size: 1.2em;
    /*font-weight: bold;*/
    border: 2px solid #EF9540;
    color: #fff;
    background: #EF9540;
    padding: 0.1em 1em;
}
.bt-reset:hover, .bt-reset:focus{
  background:#e37b00;
  border:2px solid #e37b00;
  outline:none;
}

.form-register .slt{
  font-size:1.6em;
}

/*-----BUY-----////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////*/


.btn-o{
    padding: 3px !important;
    margin: 2px !important;
}

.table-buy{
  margin-top: 10px;
}

.form-addbuy{
  font-family: 'Boon-0.6' !important;
  font-size:1.3em !important;
}

.grid-form input[type="text"],.grid-form  select {
    font-size: 18px !important;
}
.form-addbuy ul{
  list-style:none;
  margin: 0;
  padding: 0.2em 1em;
}

.form-addbuy .txt-buy{
  width:50% !important;
  font-size:1.0em;
  border:1px solid #dde3ec;
  margin:0.2em 0;
  padding:0 0.5em;
}
.form-addbuy .txt-buy:focus{
  outline:none;
  border:1px solid #ffd7a9;
}

.row-0{
  margin-right: 0 !important;
  margin-left: 0 !important;
}

.grid-form [data-row-span] [data-field-span] label:first-child {
    font-size: 0.8em !important;

}

.panel{
  font-size: 1.2em !important;
}
.panel-heading {
    background-color: #f6bb42;
    border: 1px solid #f6bb42;
}

.panel-title span{
  font-size: 1.2em;
}

.label {
  padding: .3em .6em .3em !important;
  font-size: 85% !important;
}
/*-----STOCK-----////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////*/
.search-box{
  font-size: 14px !important;
  height: 34px !important;
}
.btn-plus{
    padding: 4px 4px !important;
}

.btn-seach{
  font-size: 14px !important;
}

.lb-plus{
  padding: 0.2em 0.2em 0.1em !important;
}

.form-addproduct{
  font-family: 'Boon-0.6';
  margin:0.5em 2em 2em 2em;
  padding: 10px 10px;
  
}

.form-addproduct input[type=text]{
  width:35%;
  font-size:1.2em;
  border:1px solid #dde3ec;
  margin:0.2em 0;
  padding:0 0.5em;
}
.form-addproduct input[type=text]:focus{
  outline:none;
  border:1px solid #ffd7a9;
}

.form-addproduct .slt{
  width:35%;
  font-size:1.2em;
  font-family: 'Boon-0.6';
}

.form-addproduct ul{
  list-style:none;
  margin: 0;
    padding: 0.2em 1em;
}

.form-addproduct input[type=date] {
    width: 35%;
    font-size: 1.2em;
    border: 1px solid #dde3ec;
    margin: 0.2em 0;
}

.form-addproduct input[type=number] {
    width: 10%;
    font-size: 1.2em;
    border: 1px solid #dde3ec;
    padding: 0;
}

@media (max-width: 767px) {
  .nav-side-menu {
    position: relative;
    width: 100%;
    margin-bottom: 10px;
  }
  .nav-side-menu .toggle-btn {
    display: block;
    cursor: pointer;
    position: absolute;
    right: 10px;
    top: 10px;
    z-index: 10 !important;
    padding: 3px;
    background-color: #ffffff;
    color: #000;
    width: 40px;
    text-align: center;
  }
  .brand {
    text-align: left !important;
    font-size: 22px;
    padding-left: 20px;
    line-height: 50px !important;
  }

}




@media (min-width: 0px) {
  .nav-side-menu .menu-list .menu-content {
    display: block;
  }

}

@media only screen and (max-device-width: 767px) and (min-device-width: 320px){
.col-xs-12{
	padding-right:0px !important;
}

.header{
	display:none !important;  
}

.nav-side-menu {
  width:100%;
}

.content{
	margin-top:0px;	
}
/*-----BUY-----////////////////////////////////////////////////////////////////////////////////////////////////////////////*/
.row2{
  margin-right: 0 !important;
}

.form-addbuy .txt-buy{
  width:100%;
}

.panel{
  font-size: 1.0em;
}

.panel-title span{
  font-size: 1.0em;
}

.fa-none{
 display: none !important;
}

.form-center{
  
}

/*-----STOCK----////////////////////////////////////////////////////////////////////////////////////////////////////////////*/
.form-addproduct input[type=text]{
  width:100%;
}

.form-addproduct .slt{
  width:100%;
}

/*-------Alert------//////////////////////////////////////////////////////////////////////////////////////////////////*/


}